{
    "name": "react-redux-hot-boilerplate",
    "version": "0.0.1",
    "description": "React redux boilerplate with hot reloading, react-router, css modules and simple localization provider",
    "main": "index.js",
    "scripts": {
        "dev": "node ./server/development.js",
        "prod": "node ./node_modules/webpack/bin/webpack.js -p --progress --config ./webpack.config.prod.js && node ./server/production.js",
        "start": "node ./server/production.js --color=always"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/martyan/react-redux-hot-boilerplate.git"
    },
    "keywords": [
        "react",
        "redux",
        "boilerplate",
        "hot",
        "reloading",
        "router",
        "sass",
        "css",
        "modules",
        "localization"
    ],
    "author": "Martin Juzl",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/martyan/react-redux-hot-boilerplate/issues"
    },
    "homepage": "https://github.com/martyan/react-redux-hot-boilerplate#readme",
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "connected-react-router": "^4.4.1",
        "express": "^4.16.3",
        "history": "^4.7.2",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-helmet": "^5.2.0",
        "react-redux": "^5.0.7",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.1.5",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.2",
        "babel-plugin-react-css-modules": "^3.4.2",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.0",
        "eslint": "^5.6.0",
