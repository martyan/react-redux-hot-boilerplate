{
  "name": "react-redux-hot-boilerplate",
  "version": "0.0.1",
  "description": "React redux boilerplate with hot reloading, react-router, css modules and simple localization provider",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --content-base src --inline --config webpack.config.dev.js",
    "prod": "node ./node_modules/webpack/bin/webpack.js -p --progress --config ./webpack.config.prod.js",
    "start": "node ./server/index.js --color=always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martyan/react-redux-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "hot",
    "reloading",
    "router",
    "sass",
    "css",
    "modules",
    "localization"
  ],
  "author": "Martin Juzl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martyan/react-redux-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/martyan/react-redux-hot-boilerplate#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "connected-react-router": "^4.4.1",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.56",
    "@babel/polyfill": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-react": "^7.0.0-beta.56",
    "autoprefixer": "^9.1.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-react-css-modules": "^3.4.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "postcss-scss": "^2.0.0",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5"
  }
}
